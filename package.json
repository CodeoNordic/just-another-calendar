{
  "private": true,
  "source": "src/index.html",
  "publicUrl": "./",
  "context": "browser",
  "targets": {
    "module": false,
    "types": false
  },
  "scripts": {
    "start": "parcel --no-cache --no-source-maps --no-hmr",
    "build": "rm -rf .parcel-cache dist && parcel build --no-cache",
    "upload": "bun upload.js",
    "upload:clean": "bun run build && bun run upload"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "engines": {
    "node": ">=20"
  },
  "browserslist": "> 0.5%, last 2 versions, not IE 9-11, not dead",
  "dependencies": {
    "open": "^10.1.0",
    "preact": "^10.19.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@parcel/optimizer-data-url": "^2.12.0",
    "@parcel/transformer-image": "^2.12.0",
    "@parcel/transformer-sass": "^2.11.0",
    "@parcel/transformer-svg-react": "^2.12.0",
    "@parcel/transformer-typescript-tsc": "^2.11.0",
    "@parcel/validator-typescript": "^2.11.0",
    "@types/node": "^20.11.16",
    "cssnano": "^6.0.3",
    "parcel": "^2.12.0",
    "postcss": "^8.4.35",
    "posthtml-doctype": "^1.1.1",
    "typescript": "^5.3.3"
  }
}
